{
  "name": "remote-ftp",
  "main": "./lib/remote-ftp",
  "version": "0.1.0",
  "description": "Enable browsing remote FTP just like the built-in Tree View. Require a project",
  "activationEvents": [
      "remote-ftp:write-config",
      "remote-ftp:toggle",
      "remote-ftp:add-file",
      "remote-ftp:add-folder",
      "remote-ftp:refresh",
      "remote-ftp:move",
      "remote-ftp:delete",
      "remote-ftp:open",
      "remote-ftp:open-active"
  ],
  "repository": "https://github.com/atom/remote-ftp",
  "license": "MIT",
  "engines": {
    "atom": "*"
  },
  "dependencies": {
      "ftp": "*",
      "mkdirp": "*",
      "theorist": "1.x",
      "underscore": "1.x"
  }
}
